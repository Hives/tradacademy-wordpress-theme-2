:@template"– /*
Theme Name: Paul2
Description: Pauly's Second Trad Academy theme.
Author: Pablo
Version: 0.4
*/

$page-width: 960px;
$gutter: 20px;
$number-of-columns: 12;

$column-width: ($page-width - ($number-of-columns * $gutter))/$number-of-columns;

// $side-margin: $column-width + $gutter/2;
$side-margin: $gutter;

@mixin column-span($n) {
	width: ($n * $column-width) + (($n - 1) * $gutter);
}

@mixin box-shadow($x, $y, $blur, $spread, $color) {
    -webkit-box-shadow: $x $y $blur $spread $color;
	   -moz-box-shadow: $x $y $blur $spread $color;
            box-shadow: $x $y $blur $spread $color;
}

$light-grey: #aaaaaa;
$dark-grey: #6a6a6a;
$highlight: #873209;

$dark-green: #59682f;
$cream: #e3cd9c;
$dark-cream: #c9ba8b;

$burgundy: #780505;

$colour1: #EBD5D1;
$colour2: #D8EEEE;
$colour3: #91CF93;
$colour4: #FAF176;
$colour5: #F9B262;

@import "fonts/patua/stylesheet.css";
@import "fonts/pt-sans/stylesheet.css";
@import "fonts/contra/stylesheet.css";
@import "fonts/cabin/stylesheet.css";

/*-----------------------------------------------
	Table of contents

	$1. Typography
	$2. Layout
	$3. Bad browser styles
-------------------------------------------------*/

/*-----------------------------------------------
	$1. Typography
-------------------------------------------------*/

a {
	text-decoration: none;
	&:hover {
		text-decoration: underline;
	}
}

html {
	font-family: "CabinRegular", Sans-Serif;
}

strong {
	font-family: "CabinBold", Sans-Serif;
}

h1, h2, h3 {
	font-family: "CabinBold", Serif;
	font-weight: normal;
	a {
		color: inherit;
	}
}

#site-title {
	margin: 0;
	font-size: 100px;
	text-shadow: 3px 3px 0px $dark-grey;
	line-height: 0.8em;
	background-image: url("img/trad-academy-logo.png");
	background-size: contain;
	background-repeat: no-repeat;

	height: 100px;
	width: 565.5px;
	a:hover {
		text-decoration: none;
	}
}
#site-description {
	display: none;
	font-family: "CabinRegular", Serif;
	font-size: 24px;
	color: $dark-green;
}

.course-info {
	font-family: "CabinItalic"
}

.course-summary > div {
	font-size: 0.9em;
}

a {
	color: $burgundy;
}

/*-----------------------------------------------
	$2. Basic layout
-------------------------------------------------*/

* {
	-webkit-box-sizing: border-box;
	   -moz-box-sizing: border-box;
	        box-sizing: border-box;
}

html {
	background-image: url('img/william_morris_wallpaper.jpg');
}

body {
	-webkit-box-sizing: content-box;
	   -moz-box-sizing: content-box;
	        box-sizing: content-box;

	border-left: thin solid black;
	border-right: thin solid black;
	margin: 0 auto;
	width: $page-width + 2*$gutter;
	@include box-shadow(0, 0, 10px, 5px, rgba(0,0,0,0.3));
	background-color: white;
}

div.wp-caption {
	width: auto!important;
	padding: 1px;
	border: thin solid $light-grey;
	img {
		display: block;
	}
}

p img {
	padding: 1px;
	border: thin solid $light-grey;
	display: block;
	margin: $gutter/2 0;
}

#masthead {
	// height: 200px;
	width: 100%;

	// display: table;
	position: relative;
	// padding: 0 $side-margin + $gutter/2;
	background-repeat: no-repeat;

	// border-bottom: thin solid black;

	& > div {
		// display: table-cell;
		// vertical-align: bottom;
		padding: $gutter*2 $side-margin+$gutter;
	}

	#mascot {
		width: 140px;
		position: absolute;
		bottom: 15px;
		right: $side-margin + $gutter/2 + ((3 * $column-width + 2 * $gutter) - 130px)/2;
	}

}

#site-navigation {
	position: relative;
	z-index: 1;

	width: 100%;

	background-color: $burgundy;
	color: white;

	padding: 0 $side-margin + $gutter/2;

	> h2 {
		display: none;
	}

	ul {
	    margin: 0;
	    padding: 0;
	    list-style: none;
	}
	ul li {
		font-family: "CabinMedium";
	    position: relative;
	}
	& > ul > li {
		display: inline-block;
	}
	li {
		display: block;
	}
	li ul {
	    display: none;
	}
	ul li a {
	    display: block;
	    text-decoration: none;
	    color: inherit;
	    font-size: 18px;
	    padding: $gutter/2;
	    line-height: 1em;
	    // padding: 5px 15px 5px 15px;
	    // background: #1e7c9a;
	    // margin-left: 1px;
	    white-space: nowrap;
	}
	ul li ul li a {
	    font-family: "CabinRegular";
		font-size: 14px;
	}
	ul li a:hover {
		background: $cream;
	}
	li:hover {
		ul {
		    display: block;
		    position: absolute;
		}
		li {
		    float: none;
		}
		a {
			background: $cream;
		}
		hr {
			height: 1px;
			margin: 0;
			border: 0;
			display: block;
			background-color: $dark-cream;
			color: $dark-cream;
			font-size: 10px;
		}
		li a:hover {
		    background: $dark-cream;
		}
	}
}

.carousel-left {
	float: left;
	@include column-span(9);
	margin-top: $gutter/2;
	margin-bottom: $gutter/2;
	margin-left: -$gutter/2;
	background-color: pink;
	.carousel-element {
		position: relative;
		img {
			float: left;
		}
		.carousel-text {
			clear: both;
			h2 {
				margin-top: 0;
			}
			padding: $gutter/2;
			background-color: $cream;

		}
	}
}

.carousel-right {
	background-color: $cream;
	position: relative;
	width: 100%;
	height: 350px;
	padding: $gutter/2 800px $gutter/2 90px;
	clear: both;
	img {
		position: absolute;
		top: 0;
		right: 0;
	}
}

.carousel-full {
	// padding-top: 5px;
	// padding-bottom: 5px;
	-webkit-box-sizing: content-box;
	   -moz-box-sizing: content-box;
	        box-sizing: content-box;
	// border-bottom: thin solid black;
	// border-top: thin solid black;
	position: relative;
	width: 100%;
	height: 350px;
	clear: both;
	.carousel-element {
		height: 100%;
		padding: $gutter;
	}
	.carousel-text {
		position: relative;
		background-color: rgba(255,255,255,0.6);
		@include column-span(3);
		height: 100%;
		float: right;
		margin-right: $gutter/2;
		padding: $gutter;
		// border-left: thin solid black;
		// border-right: thin solid black;
		z-index: 1;
		h2 {
			margin-top: 0;
		}
	}
	img {
		position: absolute;
		top: 0;
		left: 0;
		z-index: 0;
	}
}


#carousel {
	position: relative;
	z-index: 0;

	@include column-span(12);
	margin: $gutter $side-margin + $gutter/2;
	// border: $gutter/2 solid $highlight;

	@include box-shadow($gutter/4, $gutter/4, $gutter/2, 0, rgba(0,0,0,0.4));

	height: 300px;
	float: left;
	overflow: hidden;
	img {
		position: absolute;
		top: 0;
		width: 100%;
		z-index: 0;
		opacity: 0;

		&.previous {
			z-index: 1;
			opacity: 1;
		}
		&.current {
			z-index: 2;
			opacity: 1;
			-webkit-transition: opacity .5s;
			   -moz-transition: opacity .5s;
			    -ms-transition: opacity .5s;
			     -o-transition: opacity .5s;
			        transition: opacity .5s;
		}
	}
}

main {
	@include column-span(9);
	margin-left: $side-margin + $gutter/2;
	margin-bottom: 50px;
	float: left;
	padding: $gutter/2;
}

article.course header > * {
	margin-bottom: $gutter/2;
}

.social-media-buttons {
	height: 29px;
	> div, > iframe {
		font-size: 1px;
		display: inline-block;
	}
}

.location {
	clear: both;
	> div {
		position: relative;
	}
	.address {
		width: 4 * $column-width + 2.5 * $gutter;
		height: 100%;
		float: right;
		p {
			margin-top: 0;
		}
	}
	.google-map {
		width: 5 * $column-width + 3.5 * $gutter;
		height: 250px;
		float: left;
	}
	.reset-map {
		position: absolute;
		bottom: 0;
		left: 5 * $column-width + 3.5 * $gutter;
		margin-left: $gutter;
		target-new: tab;
	}
}


#sidebar {
	@include column-span(3);
	margin: $gutter $side-margin + $gutter/2 $gutter 0;
	height: 100%;
	float: right;
	background-color: #eee;

	> h2 {
		display: none;
	}

	> section {
		margin-bottom: $gutter;
		padding: $gutter/2;
		> h3 {
			margin-top: 0;
		}
	}

	input[type=text] {
		width: 100%;
	}

	&:last-child {
		margin-bottom: 10px;
	}

	.cat {
		display: block;
		margin: 0 auto;
		opacity: 0.1;
	}

}

.widget_ta_social_media {
	.fb-container {
		height: 50px;
		overflow: hidden;
	}
	.fb-pic {
		width: 50px;
		height: 50px;
		float: left;
		margin-right: $gutter/2;
	}
	a.fb-link {
		display: block;
		font-family: 'PTSansBold';
		color: #3B5998;
	}
	.fb-like {
		height: 29px;
	}
}

.widget_ta_calendar {
	.course-summary {
		margin-top: $gutter/2;
		padding-bottom: $gutter/2;
		border-bottom: thin solid rgba(0,0,0,0.2);
		&:last-child {
			border: 0;
			padding: 0;
		}
		h4 {
			margin: 0;
		}
	}
}

footer {
	font-size: 0.8em;
	clear: both;
	margin: 0 $side-margin;
	padding: $gutter/2;
	border-top: thin solid $light-grey;
	text-align: center;
}

